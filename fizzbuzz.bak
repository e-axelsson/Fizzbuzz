#lang racket
(define (divisible-by? num div symbol)
  (if (zero? (modulo num div))
      symbol
      #f))   
(define (fizzbuzz num)
  (or
   (divisible-by? num 15 'FizzBuzz)
   (divisible-by? num 3 'Fizz)
   (divisible-by? num 5 'Buzz)
   num))

;Runs fizzbuzz on 1 to 100
(map fizzbuzz (build-list 100 (lambda (x) (add1 x))))